{
  "name": "Un-Censor study.com",
  "description": "Un-Censor (part of) the lesson on study.com pages.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/study_com16.png",
      "32": "/images/study_com32.png",
      "48": "/images/study_com48.png",
      "128": "/images/study_com128.png", 
      "1024": "/images/study_com1024.png"
    }
  },
  "icons": {
    "16": "/images/study_com16.png",
    "32": "/images/study_com32.png",
    "48": "/images/study_com48.png",
    "128": "/images/study_com128.png",
    "1024": "/images/study_com1024.png"
  }
}